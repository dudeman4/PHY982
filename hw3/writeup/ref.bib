@article{BOHNE1972,
title = "Study of the (d, n) AND (d, p) reactions on 15N",
journal = "Nuclear Physics A",
volume = "196",
number = "1",
pages = "41 - 57",
year = "1972",
note = "",
issn = "0375-9474",
doi = "http://dx.doi.org/10.1016/0375-9474(72)90947-5",
url = "http://www.sciencedirect.com/science/article/pii/0375947472909475",
author = "W. Bohne and J. Bommer and H. Fuchs and K. Grabisch and H. Kluge and G. Röschert",
keywords = "Nuclear reactions",
abstract = "Angular distributions of the reactions (d, d0), (d, p) and (d, n) on 15N were measured at deuteron energies of 5, 5.5 and 6 MeV. Excitation functions were obtained for the reactions (d, do) and (d, p) from 4.25 MeV to 6.25 MeV deuteron energy. The experimental data were analysed with DWBA and Hauser-Feshbach calculations to yield spectroscopic factors. These are compared with results of previous experiments and shell-model calculations. The p-1d32 configuration is strongly mixed with the states of the p-1(d52, s12) configurations; its inclusion therefore is necessary for a correct theoretical description of low-lying T = 0 states in 16O with negative parity. The experimental strengths of the 1d52 and 2s12 transitions to the lowest T = 1 states in 16O and 16N exhaust only about 60 \% of the total one-particle strength in contrast to existing nuclear structure calculations of A = 16 nuclei."
}

